<configuration>

	<!-- DTO 클래스 별칭부여 -->
	<typeAliases>
	    <typeAlias alias="ContentCommand" 
	                    type="com.content.domain.ContentCommand"/>
	</typeAliases>
	
	
	<!-- 접속 후 불러올 파일의 리스트를 적어주는 부분 -->
